{
    "Serilog": {
        "MinimumLevel": {
            "Default": "Information",
            "Override": {
                "Aksio": "Debug",
                "Microsoft": "Warning",
                "System": "Information"
            }
        },
        "WriteTo": [
            {
                "Name": "Console",
                "Args": {
                    "formatter": "Aksio.Applications.Serilog.RenderedCompactJsonFormatter, Aksio.Applications.Serilog"
                }
            }
        ]
    },
    "AllowedHosts": "*"
}